{
  "name": "hayum",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "aws-sdk": "^2.188.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "halogen": "^0.2.0",
    "include-media": "^1.4.9",
    "react": "^16.2.0",
    "react-css-modules": "^4.7.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.11",
    "react-redux": "^7.0.3",
    "react-router-dom": "^4.2.2",
    "react-router-transition": "^1.2.0",
    "react-tabs": "^2.2.1",
    "reapop": "^1.2.0",
    "recompose": "^0.26.0",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "yarn server:start",
    "cra:start": "node scripts/start.js",
    "cra:build": "node scripts/build.js",
    "cra:test": "node scripts/test.js --env=jsdom",
    "server:start": "NODE_ENV=production node ./build/main.js",
    "backpack:dev": "NODE_ENV=development DEBUG=HAYUM:* backpack dev",
    "backpack:build": "NODE_ENV=production backpack build",
    "backpack:debug": "NODE_ENV=development backpack dev $NODE_DEBUG_OPTION",
    "build:all": "yarn cra:build && yarn backpack:build",
    "build:start": "yarn build:all && yarn server:start",
    "postinstall": "yarn build:all"
  },
  "devDependencies": {
    "@types/react-css-modules": "^4.6.2",
    "@types/react-redux": "^7.0.9",
    "autoprefixer": "7.1.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "backpack-core": "^0.5.0",
    "body-parser": "^1.18.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.8",
    "dotenv": "4.0.0",
    "eslint": "^4.16.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "20.0.4",
    "mini-css-extract-plugin": "^0.7.0",
    "mongoose": "^5.0.0-rc2",
    "morgan": "^1.9.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.7",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "0.19.0",
    "ts-loader": "^6.0.1",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
